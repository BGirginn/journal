<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <!-- Keep cloud backup and device transfer disabled for app data. -->
    <cloud-backup>
        <exclude domain="root" path="." />
    </cloud-backup>
    <device-transfer>
        <exclude domain="root" path="." />
    </device-transfer>
</data-extraction-rules>
